{
  "name": "bard_app",
  "version": "0.2.17",
  "description": "Aplicacion de estadisticas para league of legends",
  "author": "fabran99",
  "main": "./public/electron.js",
  "repository": "https://github.com/fabran99/LolHelperFrontend",
  "build": {
    "appId": "com.electron.bardapp",
    "win": {
      "icon": "build/icon.ico"
    },
    "productName": "Bard App",
    "nsis": {
      "installerIcon": "build/installerIcon.ico",
      "runAfterFinish": false
    }
  },
  "homepage": "./",
  "dependencies": {
    "@material-ui/core": "^4.9.14",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.19.2",
    "chart.js": "^2.9.3",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "electron-is-dev": "^1.2.0",
    "electron-updater": "^4.3.4",
    "lcu-connector": "^2.1.3",
    "league-connect": "^2.0.1",
    "lodash": "^4.17.15",
    "moment": "^2.27.0",
    "react": "^16.13.1",
    "react-chartjs-2": "^2.9.0",
    "react-dom": "^16.13.1",
    "react-hextech": "^1.2.6",
    "react-icons": "^3.10.0",
    "react-particles-js": "^3.2.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-toastify": "^6.0.5",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "request-promise": "^4.2.5",
    "reselect": "^4.1.6",
    "update-electron-app": "^1.5.0"
  },
  "scripts": {
    "react-start": "react-scripts --openssl-legacy-provider start",
    "react-build": "react-scripts --openssl-legacy-provider build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "build": "npm run react-build && npm run electron-build",
    "start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"electron .\"",
    "ship": "electron-builder -p always -c electron-builder.yml",
    "electron-start": "electron ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "electron": "^8.2.3",
    "electron-builder": "^22.5.1",
    "wait-on": "^4.0.2"
  }
}
